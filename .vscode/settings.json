{
    "eslint.run": "onType",
    "eslint.options": {
        "overrideConfigFile": ".eslintrc.js",
        "extensions": [
            ".js",
            ".ts",
            ".jsx",
            ".tsx"
        ]
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "search.followSymlinks": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/tmp": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/tmp/**": true,
        "**/bower_components/**": true,
        "**/dist/**": true
    },
    "cSpell.words": [
        "daterange",
        "edate",
        "forex",
        "multilines",
        "parens",
        "RATEFORCECASTINGMODEL",
        "sdate"
    ],
}
